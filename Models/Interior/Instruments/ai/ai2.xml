<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>ai2.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>face</object-name>
    <object-name>ball</object-name>
    <object-name>aircraft</object-name>
   

    
  </animation>

 

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>ball</object-name>
    <object-name>aircraft</object-name>  
    <emission>
            <red>  0.0175</red>
            <green>0.0175</green>
            <blue> 0.0175</blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ball</object-name>
    <property>/orientation/roll-deg</property>
    <factor>1</factor>
    <center>
      <x-m> -0.062 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.000 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>  

  <animation>
    <type>rotate</type>
    <object-name>ball</object-name>
    <property>/orientation/pitch-deg</property>
    <factor>1</factor>
    <center>
      <x-m> -0.062 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.000 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>  


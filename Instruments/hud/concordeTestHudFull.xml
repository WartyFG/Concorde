<label>
	<x>-50</x> <y>-20</y> <width>100</width> <height>6</height>
	<box>true</box> <halign>center</halign>
	<format>!!! SPEED BRAKE !!!</format>
	<condition>
		<equals>
			<property>/controls/flight/speedbrake</property><value>1</value>
